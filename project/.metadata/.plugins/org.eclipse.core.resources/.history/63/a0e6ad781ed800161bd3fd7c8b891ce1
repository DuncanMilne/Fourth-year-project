import gurobi.*;
import java.io.*;
import java.util.*;

/**
* <p> This class contains the model unit for the SPA_IP_HR_Alt IP for solving SPA instances with student 
* and lecturer preferences over projects. </p>
*
* @authors Duncan Milne, based on a model unit generously gifted by Frances Cooper.
*/
public class IP_model_unit {
	Project proj;
	GRBVar studentPrefVar;

	public IP_model_unit(GRBModel grbmodel, Project proj, int rank, String name) throws GRBException {
		this.proj = proj;
		
		studentPrefVar = grbmodel.addVar(0.0, 1.0, 0.0, GRB.BINARY, "pref" + name);  
	}
}